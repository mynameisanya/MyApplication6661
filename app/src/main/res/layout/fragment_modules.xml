<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/background2_"
    android:layout_height="match_parent"
    tools:context=".ui.modules.ModulesFragment">


    <ImageButton
        android:id="@+id/distress_tolerance"
        android:layout_width="280dp"
        android:layout_height="123dp"
        android:layout_marginTop="256dp"
        android:src="@drawable/distress_tolerance_image"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/mindfulness"
        android:layout_width="308dp"
        android:layout_height="131dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="64dp"
        tools:src="@tools:sample/backgrounds/scenic" />

    <ImageButton
        android:id="@+id/emotional_regulation"
        android:layout_width="276dp"
        android:layout_height="113dp"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="407dp"
        tools:src="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/interpersonal_effectiveness"
        android:layout_width="324dp"
        android:layout_height="179dp"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="536dp"
        tools:src="@tools:sample/avatars" />


</androidx.constraintlayout.widget.ConstraintLayout>